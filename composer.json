{
    "name": "thepublicgood/payfast",
    "description": "Simple PayFast integration",
    "keywords": ["payfast", "commerce"],
    "license": "MIT",
    "authors": [
        {
            "name": "Warrick Bayman",
            "email": "makers@thepublicgood.dev"
        }
    ],
    "require": {
        "php": ">=8.2",
        "ext-json": "*",
        "guzzlehttp/guzzle": "^7.5",
        "illuminate/collections": "^10.0",
        "thepublicgood/yerp": "^0.0.4"
    },
    "require-dev": {
        "laravel/pint": "^1.11",
        "orchestra/testbench": "^8.8",
        "pestphp/pest": "^2.14",
        "phpunit/phpunit": "^10.3",
        "spatie/ray": "^1.37"
    },
    "autoload": {
        "psr-4": {
            "TPG\\PHPayfast\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "TPG\\PHPayfast\\Tests\\": "tests/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.x-dev"
        }
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
