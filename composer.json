{
    "name": "thepublicgood/payfast",
    "description": "Simple PayFast integration",
    "keywords": ["payfast", "commerce"],
    "license": "MIT",
    "authors": [
        {
            "name": "Warrick Bayman",
            "email": "makers@thepublicgood.dev"
        }
    ],
    "require": {
        "php": "^8.1",
        "ext-json": "*",
        "guzzlehttp/guzzle": "^7.4",
        "illuminate/collections": "^8.0|^9.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5",
        "roave/security-advisories": "dev-master",
        "spatie/ray": "^1.10",
        "symfony/var-dumper": "^5.2|^6.0",
        "orchestra/testbench": "^6.24|^7.3"
    },
    "autoload": {
        "psr-4": {
            "TPG\\PayFast\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "TPG\\PayFast\\Tests\\": "tests/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.x-dev"
        },
        "laravel": {
            "providers": [
                "TPG\\PayFast\\ServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
